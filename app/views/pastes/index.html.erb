<p id="notice"><%= notice %></p>

<h1>Listing Pastes</h1>

<table class="table ">
  <thead>
    <tr>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pastes.each do |paste| %>
      <tr>
        <td><%= paste.id %></td>
        <td><%= link_to 'Show', paste, class: "btn btn-default" %></td>
        <td><%= link_to 'Edit', edit_paste_path(paste), class: "btn btn-default" %></td>
        <td><%= link_to 'Destroy', paste, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-default" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Paste', new_paste_path, class: "btn btn-default" %>
